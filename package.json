{
  "name": "vite-plugin-triggerkit",
  "version": "1.3.2",
  "description": "A plugin to connect SvelteKit functions to your trigger.dev tasks",
  "homepage": "https://github.com/EnoughXP/vite-plugin-triggerkit#readme",
  "keywords": [
    "vite",
    "vite-plugin",
    "svelte",
    "sveltekit",
    "trigger.dev",
    "functions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EnoughXP/vite-plugin-triggerkit.git"
  },
  "bugs": {
    "url": "https://github.com/EnoughXP/vite-plugin-triggerkit/issues"
  },
  "license": "MIT",
  "author": "Jason Gutierrez",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./trigger": {
      "types": "./dist/trigger.d.ts",
      "import": "./dist/trigger.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "@trigger.dev/sdk": "^3.0.0",
    "esbuild": "^0.19.0",
    "vite": ">=6.0.0"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^6.21.0",
    "@typescript-eslint/types": "^6.21.0",
    "fast-glob": "^3.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@trigger.dev/build": "^3.3.16",
    "@trigger.dev/sdk": "^3.3.16",
    "@types/node": "^22.12.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitest/coverage-v8": "^3.0.6",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "fast-glob": "^3.3.3",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "svelte": "^5.0.0",
    "trigger.dev": "^3.3.16",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.6"
  },
  "scripts": {
    "clean": "node -e \"fs.rmSync('dist', { recursive: true, force: true })\"",
    "prebuild": "pnpm run clean",
    "build": "vite build && pnpm run package",
    "package": "svelte-kit sync && svelte-package && publint && cp README.md dist/",
    "changeset": "changeset",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "version": "changeset version",
    "release": "pnpm run package && changeset publish"
  }
}