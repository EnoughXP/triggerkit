{
  "name": "vite-plugin-triggerkit",
  "version": "1.2.0",
  "description": "A plugin to connect SvelteKit functions to your trigger.dev tasks",
  "keywords": [
    "svelte",
    "trigger.dev"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EnoughXP/vite-plugin-triggerkit.git"
  },
  "license": "MIT",
  "author": "Jason Gutierrez",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./trigger": {
      "types": "./dist/trigger.d.ts",
      "import": "./dist/trigger.js"
    }
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "bin": {
    "triggerkit": "./dist/cli/index.js"
  },
  "peerDependencies": {
    "@trigger.dev/sdk": "^3.3.16",
    "esbuild": "^0.25.0",
    "vite": ">=6.0.0"
  },
  "dependencies": {
    "fast-glob": "^3.3.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@trigger.dev/build": "^3.3.16",
    "@trigger.dev/sdk": "^3.3.16",
    "@types/node": "^22.12.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@typescript-eslint/types": "^6.21.0",
    "@typescript-eslint/typescript-estree": "^6.21.0",
    "@vitest/coverage-v8": "^3.0.6",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "fast-glob": "^3.3.3",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.2",
    "recast": "^0.23.9",
    "rollup-plugin-visualizer": "^5.14.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "trigger.dev": "^3.3.16",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.6"
  },
  "scripts": {
    "clean": "node -e \"fs.rmSync('dist', { recursive: true, force: true })\"",
    "prebuild": "pnpm run clean",
    "build": "vite build && pnpm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "trigger:dev": "pnpm dlx trigger.dev@latest dev",
    "trigger:deploy": "pnpm dlx trigger.dev@latest deploy",
    "trigger:update": "pnpm dlx trigger.dev@latest update",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "format": "prettier --plugin-search-dir . --write .",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "package": "svelte-kit sync && svelte-package && publint && cp README.md dist/",
    "preview": "vite preview",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm run build && changeset publish"
  }
}